<?xml version="1.0" encoding="UTF-8"?>
<manifest>

   <!-- Github -->
   <remote name="me"
           revision="x"
           fetch="https://github.com/MikereDD/" />

   <remote name="du"
            revision="q10x"
            fetch="https://github.com/DirtyUnicorns/" />

   <!-- Removes -->
   <remove-project name="Aqua-devices/device_google_crosshatch" />
   <remove-project name="Aqua-devices/device_google_crosshatch-sepolicy" />
   <remove-project name="Aqua-devices/kernel_google_bluecross" />
   <remove-project name="Aqua-devices/proprietary_vendor_google" />

   <!-- Device -->
   <project name="device_google_crosshatch" path="device/google/crosshatch" remote="me" />
   <project name="android_device_google_crosshatch-sepolicy" path="device/google/crosshatch-sepolicy" remote="du" />

   <!-- Kernel -->
   <project name="kernel_google_bluecross" path="kernel/google/bluecross" remote="me" />

   <!-- Vendor -->
   <project name="android_vendor_google" path="vendor/google" remote="du" />

</manifest>
