<?xml version="1.0" encoding="UTF-8"?>
<manifest>

    <!-- EXAMPLE LOCAL MANIFEST -->

 <!-- fetch from github and bitbucket for use with custom user repos -->

  <remote name="bb"
          fetch="https://bitbucket.org/" />
  <remote name="gh"
          fetch="https://github.com/" />
 <!-- use your personal repos via ssh instead of https -->
  <remote name="me"
          fetch="ssh://git@github.com/AddUserName/" />

 <!-- remove repos -->
  <remove-project name="" />
  <remove-project name="" />
  <remove-project name="" />
  <remove-project name="" />

 <!-- add/replace custom repos -->
  <project path="" name=""  remote="bb" revision="" />
  <project path="" name=""  remote="bb" revision="" />
  <project path="" name=""  remote="gh" revision="" />
  <project path="" name=""  remote="gh" revision="" />
  <project path="" name=""  remote="me" revision="" />
  <project path="" name=""  remote="me" revision="" />

</manifest>
